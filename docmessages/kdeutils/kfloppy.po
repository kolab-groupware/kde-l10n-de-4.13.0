#. Tag: para
#: index.docbook:80 index.docbook:96
#, no-c-format
msgid ""
"&kfloppy; Formatter is a utility that provides a straightforward graphical "
"means to format 3.5\" and 5.25\" floppy disks."
msgstr ""
"&kfloppy;, ist ein einfaches Programm, zum Formatieren von 3.5<quote>- und "
"5.25</quote>-Disketten."

#. Tag: keyword
#: index.docbook:85
#, no-c-format
msgid "<keyword>KDE</keyword>"
msgstr "<keyword>KDE</keyword>"

