#: functions.docbook:10220
#, no-c-format
msgid "T(value)"
msgstr "T(Wert)"

# |, no-fancy-quote
#. Tag: para
#: functions.docbook:10227
#, no-c-format
msgid "T(\"Calligra\") returns \"Calligra\""
msgstr "T(\"Calligra\") ergibt \"Calligra\""

#. Tag: para
#: functions.docbook:10230
#, no-c-format
msgid "T(1.2) returns \"\" (empty text)"
msgstr "T(1,2) ergibt \"\" (leerer Text)"

#. Tag: title
#: functions.docbook:10236
#, no-c-format
msgid "TEXT"
msgstr "TEXT"

#. Tag: para
#: functions.docbook:10237
#, no-c-format
msgid "The TEXT() function converts a value to text."
msgstr "Die Funktion TEXT() konvertiert einen Wert in einen Text."

