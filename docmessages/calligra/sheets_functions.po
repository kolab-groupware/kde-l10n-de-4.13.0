msgstr "TOGGLE(\"hallo\") ergibt \"HALLO\""

# |, no-fancy-quote
#. Tag: para
#: functions.docbook:10274
#, no-c-format
msgid "TOGGLE(\"HELLO\") returns \"hello\""
msgstr "TOGGLE(\"HALLO\") ergibt \"hallo\""

# |, no-fancy-quote
#. Tag: para
#: functions.docbook:10277
#, no-c-format
msgid "TOGGLE(\"HeLlO\") returns \"hElLo\""
msgstr "TOGGLE(\"HaLlO\") ergibt \"hAlLo\""

