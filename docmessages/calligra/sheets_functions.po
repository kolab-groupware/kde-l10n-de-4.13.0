#, no-c-format
msgid "Regular expression"
msgstr "Regulärer Ausdruck"

#. Tag: seg
#: functions.docbook:9894
#, no-c-format
msgid "Default value (optional)"
msgstr "Standardwert (optional)"

#. Tag: seg
#: functions.docbook:9895
#, no-c-format
msgid "Back-reference (optional)"
msgstr "Rückverweis (optional)"

# |, no-fancy-quote
#. Tag: para
#: functions.docbook:9898
#, no-c-format
msgid "REGEXP(\"Number is 15.\";\"[0-9]+\") = \"15\""
msgstr "REGEXP(\"Anzahl ist 15.\";\"[0-9]+\") = \"15\""

# |, no-fancy-quote
#. Tag: para
#: functions.docbook:9902
#, no-c-format
msgid "REGEXP(\"15, 20, 26, 41\";\"([0-9]+), *[0-9]+$\";\"\";1) = \"26\""
msgstr "REGEXP(\"15, 20, 26, 41\";\"([0-9]+), *[0-9]+$\";\"\";1) = \"26\""

#. Tag: title
#: functions.docbook:9908
#, no-c-format
msgid "REGEXPRE"
msgstr "REGEXPRE"

#. Tag: para
#: functions.docbook:9909
#, no-c-format
msgid "Replaces all matches of a regular expression with the replacement text"
msgstr ""
"Ersetzt alle Vorkommen eines regulären Ausdrucks mit dem Ersetzungstext."

