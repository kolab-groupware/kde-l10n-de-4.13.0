msgstr "<guimenu>Datei</guimenu> <guimenuitem>Schließen</guimenuitem>"

#. Tag: action
#: editor.docbook:82
#, no-c-format
msgid "Closes the active dialog"
msgstr "Schließt den aktuellen Dialog"

#. Tag: menuchoice
#: editor.docbook:85
#, no-c-format
msgid ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>S</keycap></keycombo> </"
"shortcut> <guimenu>File</guimenu> <guimenuitem>Save</guimenuitem>"
msgstr ""
"<shortcut> <keycombo action=\"simul\">&Ctrl;<keycap>S</keycap></keycombo> </"
"shortcut> <guimenu>Datei</guimenu> <guimenuitem>Speichern</guimenuitem>"

#. Tag: action
#: editor.docbook:92
#, no-c-format
msgid "Saves the active dialog"
msgstr "Speichert den aktuellen Dialog"

#. Tag: menuchoice
#: editor.docbook:95
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Save As</guimenuitem>"
msgstr ""
"<guimenu>Datei</guimenu> <guimenuitem>Speichern unter ...</guimenuitem>"

#. Tag: action
#: editor.docbook:99
#, no-c-format
msgid "Saves the active dialog with another name"
msgstr "Speichert den aktuellen Dialog mit einem anderen Namen"

#. Tag: menuchoice
#: editor.docbook:102
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Save All</guimenuitem>"
msgstr "<guimenu>Datei</guimenu> <guimenuitem>Alles Speichern</guimenuitem>"

#. Tag: action
#: editor.docbook:106
#, no-c-format
msgid "Saves all open dialogs"
msgstr "Speichert alle geöffneten Dialoge"

#. Tag: menuchoice
#: editor.docbook:109
#, no-c-format
msgid ""
"<guimenu>File</guimenu> <guimenuitem>Recently Opened Files</guimenuitem>"
msgstr ""
"<guimenu>Datei</guimenu> <guimenuitem>Kürzlich geöffnete Dateien</"
"guimenuitem>"

#. Tag: action
#: editor.docbook:113
#, no-c-format
msgid ""
"Quick list of the last several files you've opened. This list will change "
"each time you open a file that is not on it with the oldest being bumped off "
"first."
msgstr ""
"Liste der zuletzt geöffneten Dateien. Diese Liste ändert sich mit jeder "
"geöffneten Datei, die noch nicht darin enthalten war, wobei die ältest "
"geöffnete Datei herausfällt."

#. Tag: menuchoice
#: editor.docbook:116
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Exit</guimenuitem>"
msgstr "<guimenu>Datei</guimenu> <guimenuitem>Beenden</guimenuitem>"

#. Tag: para
#: editor.docbook:120
#, no-c-format
msgid "<action>Quits</action> &kommander;"
msgstr "<action>Beendet</action> &kommander;"

