#, no-c-format
msgid "<para>Map</para>"
msgstr "<para>Karte</para>"

#. Tag: para
#: index.docbook:133
#, no-c-format
msgid "Number of Neutral planets"
msgstr "Anzahl der neutralen Planeten"

#. Tag: para
#: index.docbook:134
#, no-c-format
