#, no-c-format
msgid ""
"<shortcut><keycap>P</keycap></shortcut> <guimenu>Move</guimenu> "
"<guimenuitem>Pass Move</guimenuitem>"
msgstr ""
"<shortcut><keycap>P</keycap></shortcut><guimenu>Zug</"
"guimenu><guimenuitem>Aussetzen</guimenuitem>"

#. Tag: para
#: index.docbook:236
