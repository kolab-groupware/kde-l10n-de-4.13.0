#, no-c-format
msgid ""
"<shortcut><keycap>F</keycap></shortcut> <guimenu>Game</guimenu> "
"<guimenuitem>Finish Game</guimenuitem>"
msgstr ""
"<shortcut><keycap>F</keycap></shortcut><guimenu>Spiel</"
"guimenu><guimenuitem>Spiel beenden</guimenuitem>"

