#. Tag: para
#: index.docbook:27
#, no-c-format
msgid "This documentation describes &kde;’s information center."
msgstr "Dieses Dokument beschreibt das &kde;-Infozentrum."

#. Tag: keyword
#: index.docbook:31
#, no-c-format
msgid "<keyword>KDE</keyword>"
msgstr "<keyword>KDE</keyword>"

