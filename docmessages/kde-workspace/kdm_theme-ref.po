#: theme-ref.docbook:1060
#, no-c-format
msgid "<seg>%%</seg>"
msgstr "<seg>%%</seg>"

#. Tag: seg
#: theme-ref.docbook:1061
#, no-c-format
msgid "A literal % character"
msgstr "Ein %-Zeichen"

