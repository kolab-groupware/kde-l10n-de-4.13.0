msgstr ""
"<ulink url=\"help:/dolphin/configuring-dolphin.html#preferences-dialog-"
"general\">Allgemein</ulink>: Allgemeine Einstellungen für das "
"Dateiverwaltungsprogramm"

#. Tag: para
#: index.docbook:3192
#, no-c-format
msgid ""
"<ulink url=\"help:/kcontrol/filetypes/index.html\">File Associations</"
"ulink>: Configure file associations"
msgstr ""
"<ulink url=\"help:/kcontrol/filetypes/index.html\">Dateizuordnungen</ulink>: "
"Dateizuordnungen vornehmen"

#. Tag: para
#: index.docbook:3194
#, no-c-format
msgid ""
"<ulink url=\"help:/kcontrol/trash/index.html\">Trash</ulink>: Configure "
"trash settings"
msgstr ""
"<ulink url=\"help:/kcontrol/trash/index.html\">Papierkorb</ulink>: "
"Papierkorb-Einstellungen festlegen"

#. Tag: para
