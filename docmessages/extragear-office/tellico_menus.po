#: menus.docbook:629
#, no-c-format
msgid "<guimenu>Settings</guimenu> <guimenuitem>Show Entry View</guimenuitem>"
msgstr ""
"<guimenu>Einstellungen</guimenu> <guimenuitem>Eintragsansicht anzeigen</"
"guimenuitem> "

#. Tag: para
#: menus.docbook:636
#, no-c-format
msgid ""
"This command <action>toggles the display of the <interface>Entry View</"
"interface></action>."
msgstr ""
"Dieser Befehl <action>schaltet die Anzeige der <interface>Eintragsansicht</"
"interface> an bzw. aus</action>."

#. Tag: menuchoice
#: menus.docbook:642
#, no-c-format
msgid "<guimenu>Settings</guimenu> <guimenuitem>Group Selection</guimenuitem>"
msgstr ""
"<guimenu>Einstellungen</guimenu> <guimenuitem>Gruppen-Auswahl</guimenuitem>"

#. Tag: para
#: menus.docbook:649
#, no-c-format
msgid ""
"This command <action>changes the field used to group the entries</action> in "
