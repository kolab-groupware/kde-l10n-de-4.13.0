#: index.docbook:4444
#, no-c-format
msgid ""
"<guimenu>Settings</guimenu> <guimenuitem>Configure Date &amp; Time...</"
"guimenuitem>"
msgstr ""
"<guimenu>Einstellungen</guimenu> <guimenuitem>Datum und Uhrzeit einstellen..."
"</guimenuitem>"

