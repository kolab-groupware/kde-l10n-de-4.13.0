#. Tag: refpurpose
#: man-kde4-config.1.docbook:25
#, no-c-format
msgid "Prints &kde; installation paths"
msgstr "Gibt die Installationspfade von &kde; aus"

#. Tag: cmdsynopsis
#: man-kde4-config.1.docbook:29
#, no-c-format
msgid ""
"<command>kde4-config</command> <arg>--expandvars</arg> <arg>--prefix</arg> "
"<arg>--exec-prefix</arg> <arg>--libsuffix</arg> <arg>--localprefix</arg> "
"<arg>--version</arg> <arg>--types</arg> <arg>--path <replaceable>type</"
"replaceable></arg> <arg>--userpath <replaceable>type</replaceable></arg> "
"<arg>--install <replaceable>type</replaceable></arg>"
msgstr ""
"<command>kde4-config</command> <arg>--expandvars</arg> <arg>--prefix</arg> "
"<arg>--exec-prefix</arg> <arg>--libsuffix</arg> <arg>--localprefix</arg> "
"<arg>--version</arg> <arg>--types</arg> <arg>--path <replaceable>type</"
"replaceable></arg> <arg>--userpath <replaceable>type</replaceable></arg> "
"<arg>--install <replaceable>type</replaceable></arg>"

#. Tag: title
#: man-kde4-config.1.docbook:45
#, no-c-format
msgid "Description"
msgstr "Beschreibung"

#. Tag: para
#: man-kde4-config.1.docbook:46
#, no-c-format
msgid ""
"kde4-config is a command line program used to retrieve information about "
"&kde; installation or user paths."
msgstr ""
"kde4-config ist Befehlszeilenprogramm, mit dem Informationen über die &kde;-"
"Installation oder Benutzerpfade abgerufen werden können."

#. Tag: para
#: man-kde4-config.1.docbook:49
#, no-c-format
msgid ""
"Use this program to determine where the various aspects of the &kde; "
"installation reside on your system."
msgstr ""
"Verwenden Sie dieses Programm zur Abfrage der verschiedenen Aspekte der auf "
"Ihrem System vorliegenden &kde;-Installation."

#. Tag: title
#: man-kde4-config.1.docbook:56
#, no-c-format
msgid "Options"
msgstr "Optionen"

#. Tag: option
#: man-kde4-config.1.docbook:60
#, no-c-format
msgid "--expandvars"
msgstr "--expandvars"

#. Tag: para
#: man-kde4-config.1.docbook:62
#, no-c-format
msgid "expand ${prefix} and ${exec_prefix} in output"
msgstr "${prefix} und ${exec_prefix} bei der Ausgabe expandieren"

#. Tag: option
#: man-kde4-config.1.docbook:67
#, no-c-format
msgid "--prefix"
msgstr "--prefix"

#. Tag: para
#: man-kde4-config.1.docbook:69
#, no-c-format
msgid "Compiled in prefix for &kde; libraries"
msgstr "Einkompiliertes prefix für &kde;-Bibliotheken"

#. Tag: option
#: man-kde4-config.1.docbook:74
#, no-c-format
msgid "--exec-prefix"
msgstr "--exec-prefix"

#. Tag: para
#: man-kde4-config.1.docbook:76
#, no-c-format
msgid "Compiled in exec_prefix for &kde; libraries"
msgstr "Einkompiliertes exec_prefix für &kde;-Bibliotheken"

#. Tag: option
#: man-kde4-config.1.docbook:81
#, no-c-format
msgid "--libsuffix"
msgstr "--libsuffix"

#. Tag: para
#: man-kde4-config.1.docbook:83
#, no-c-format
msgid "Compiled in library path suffix"
msgstr "Bei der Kompilierung gesetztes <quote>library path suffix</quote>"

#. Tag: option
#: man-kde4-config.1.docbook:88
#, no-c-format
msgid "--localprefix"
msgstr "--localprefix"

#. Tag: para
#: man-kde4-config.1.docbook:90
#, no-c-format
msgid "Prefix in $<envar>HOME</envar> used to write files"
msgstr "Präfix für das Schreiben von Dateien in $<envar>HOME</envar>."

#. Tag: option
#: man-kde4-config.1.docbook:95
#, no-c-format
msgid "--version"
msgstr "--version"

#. Tag: para
#: man-kde4-config.1.docbook:97
#, no-c-format
msgid "Compiled in version string for &kde; libraries"
msgstr "Einkompilierte Versionszeichenkette für &kde;-Bibliotheken"

#. Tag: option
#: man-kde4-config.1.docbook:102
#, no-c-format
msgid "--types"
msgstr "--types"

#. Tag: para
#: man-kde4-config.1.docbook:104
#, no-c-format
msgid "Print a list of all available &kde; resource types"
msgstr "Ausgabe der Liste aller verfügbaren &kde;-Ressourcetypen"

#. Tag: option
#: man-kde4-config.1.docbook:109
#, no-c-format
msgid "--path <replaceable>type</replaceable>"
msgstr "--path <replaceable>Typ</replaceable>"

#. Tag: para
#: man-kde4-config.1.docbook:111
#, no-c-format
msgid "Search path for the specified <replaceable>type</replaceable>"
msgstr "Suchpfad für den angegebenen <replaceable>Typ</replaceable>"

#. Tag: option
#: man-kde4-config.1.docbook:116
#, no-c-format
msgid "--userpath <replaceable>type</replaceable>"
msgstr "--userpath <replaceable>Typ</replaceable>"

#. Tag: para
#: man-kde4-config.1.docbook:118
#, no-c-format
msgid "User paths for the types: desktop|autostart|trash|document"
msgstr "Benutzerpfade für die Typen: desktop|autostart|trash|document"

#. Tag: option
#: man-kde4-config.1.docbook:123
#, no-c-format
msgid "--install <replaceable>type</replaceable>"
msgstr "--install <replaceable>Typ</replaceable>"

#. Tag: para
#: man-kde4-config.1.docbook:125
#, no-c-format
msgid "Prefix to install resource files to"
msgstr "Präfix, in dem Ressourcen-Dateien abgelegt werden"

#. Tag: title
#: man-kde4-config.1.docbook:133
#, no-c-format
msgid "Examples"
msgstr "Beispiele"

#. Tag: para
#: man-kde4-config.1.docbook:135
#, no-c-format
msgid "Print the top-level &kde; installation directory:"
msgstr "Ausgabe des Basisordners der &kde;-Installation:"

#. Tag: para
#: man-kde4-config.1.docbook:136
#, no-c-format
msgid ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --prefix</command> "
"</userinput>"
msgstr ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --prefix</command> "
"</userinput>"

#. Tag: para
#: man-kde4-config.1.docbook:141
#, no-c-format
msgid "Print a list of all resource types:"
msgstr "Ausgabe einer Liste aller Ressourcentypen:"

#. Tag: para
#: man-kde4-config.1.docbook:143
#, no-c-format
msgid ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --types</command> "
"</userinput>"
msgstr ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --types</command> "
"</userinput>"

#. Tag: para
#: man-kde4-config.1.docbook:148
#, no-c-format
msgid "Print the installed wallpaper path:"
msgstr "Ausgabe des Pfades des installierten Arbeitsflächenhintergrunds:"

#. Tag: para
#: man-kde4-config.1.docbook:150
#, no-c-format
msgid ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --path wallpaper</"
"command> </userinput>"
msgstr ""
"<prompt>&percnt;</prompt><userinput> <command>kde4-config --path wallpaper</"
"command> </userinput>"

