msgstr ""
"<guimenu>Einstellungen</guimenu> <guimenuitem>&kget; einrichten ...</"
"guimenuitem>"

#. Tag: action
#: index.docbook:181
#, no-c-format
msgid "Opens the &kget; configuration window"
msgstr "Öffnet den Dialog zum Einrichten von &kget;"

#. Tag: title
#: index.docbook:186
#, no-c-format
msgid "<title>Appearance</title>"
msgstr "<title>Erscheinungsbild</title>"

#. Tag: screeninfo
#: index.docbook:189
#, no-c-format
msgid "<screeninfo>Appearance</screeninfo>"
msgstr "<screeninfo>Erscheinungsbild</screeninfo>"

#. Tag: phrase
